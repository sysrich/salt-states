[Unit]
Description=Photosync
After=network-online.target

[Service]
ExecStartPre=-/usr/bin/podman pull registry.opensuse.org/home/rbrownsuse/containers/containers/photosync:latest
ExecStart=/usr/bin/podman run --rm -d --privileged registry.opensuse.org/home/rbrownsuse/containers/containers/photosync:latest

[Install]
WantedBy=multi-user.target
